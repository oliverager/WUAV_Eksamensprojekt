<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="GUI.Controller.CreateCustomerController"
            prefHeight="750.0" prefWidth="800.0">
    <children>
        <Label layoutX="50.0" layoutY="50.0" styleClass="Title" text="Opret ny Bruger" />
        <Label layoutX="50.0" layoutY="130.0" styleClass="Text_Label" text="Navn" />
        <Label layoutX="50.0" layoutY="250.0" styleClass="Text_Label" text="Kundetype" />
        <Label layoutX="450.0" layoutY="130.0" styleClass="Text_Label" text="Adresse" />
        <Label layoutX="450.0" layoutY="250.0" styleClass="Text_Label" text="CVR-nr" />
        <TextField fx:id="txtName" layoutX="50.0" layoutY="170.0" prefWidth="300.0" />
        <TextField fx:id="txtAddress" layoutX="450.0" layoutY="170.0" prefWidth="300.0" />
        <TextField fx:id="txtCvr" layoutX="450.0" layoutY="290.0" prefWidth="300.0" />
        <Button fx:id="btnCreateCustomer" layoutX="50.0" layoutY="650.0" mnemonicParsing="false" onAction="#handleCreateUser" prefHeight="50.0" prefWidth="200.0" styleClass="pane-button" text="Opret" />
        <ComboBox fx:id="cbCustomerType" layoutX="50.0" layoutY="290.0" prefHeight="36.0" prefWidth="300.0" />
    </children>
</Pane>
